{
    "roots": ["<rootDir>/src/test/"],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
        "\\.(css|less|scss|sass|svg)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": ["<rootDir>/src/setupTests.ts", "jest-plugin-context/setup"],
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    }
  }